<h1>Listing native_issue_has_asanas</h1>

<table>
  <thead>
    <tr>
      <th>Asana task</th>
      <th>Native issue</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @native_issue_has_asanas.each do |native_issue_has_asana| %>
      <tr>
        <td><%= native_issue_has_asana.asana_task %></td>
        <td><%= native_issue_has_asana.native_issue %></td>
        <td><%= link_to 'Show', native_issue_has_asana %></td>
        <td><%= link_to 'Edit', edit_native_issue_has_asana_path(native_issue_has_asana) %></td>
        <td><%= link_to 'Destroy', native_issue_has_asana, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Native issue has asana', new_native_issue_has_asana_path %>
