<h1>Listing user_has_favorite_nodes</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Node</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_has_favorite_nodes.each do |user_has_favorite_node| %>
      <tr>
        <td><%= user_has_favorite_node.user %></td>
        <td><%= user_has_favorite_node.node %></td>
        <td><%= link_to 'Show', user_has_favorite_node %></td>
        <td><%= link_to 'Edit', edit_user_has_favorite_node_path(user_has_favorite_node) %></td>
        <td><%= link_to 'Destroy', user_has_favorite_node, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User has favorite node', new_user_has_favorite_node_path %>
