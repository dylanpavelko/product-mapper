<h1>Listing Native Issues Imported from Google Sheets</h1>

<table class="table">
  <thead>
    <tr>
      <th>Native issue</th>
      <th>Google sheet</th>
      <th>External</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @issue_exists_in_google_sheets.each do |issue_exists_in_google_sheet| %>
      <tr>
        <td><%= issue_exists_in_google_sheet.native_issue.summary %></td>
        <td><%= issue_exists_in_google_sheet.google_sheet.name %></td>
        <td><%= issue_exists_in_google_sheet.external_id %></td>
        <td><%= link_to 'Show', issue_exists_in_google_sheet %></td>
        <td><%= link_to 'Edit', edit_issue_exists_in_google_sheet_path(issue_exists_in_google_sheet) %></td>
        <td><%= link_to 'Destroy', issue_exists_in_google_sheet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Issue exists in google sheet', new_issue_exists_in_google_sheet_path %>
